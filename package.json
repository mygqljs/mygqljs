{
  "name": "vite-react-ts-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:docs": "vite build --outDir docs --base https://cdn.jsdelivr.net/gh/mygqljs/mygqljs.github.io/docs/ --emptyOutDir",
    "format": "prettier --write .",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "lint-staged": "lint-staged --allow-empty",
    "prepare": "husky install",
    "preview": "vite preview",
    "preview:docs": "vite preview --outDir docs"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.1",
    "@codemirror/lang-json": "^6.0.1",
    "@mygql/codegen": "0.0.1-alpha.3",
    "@mygql/graphql": "0.0.1-alpha.3",
    "@uiw/react-codemirror": "^4.21.13",
    "classnames": "^2.3.2",
    "json5": "^2.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.5",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "lint-staged": "^13.3.0",
    "prettier": "2.8.8",
    "typescript": "5.1.6",
    "vite": "^4.4.9"
  }
}
